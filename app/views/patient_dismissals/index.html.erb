<p id="notice"><%= notice %></p>

<h1>Patient Dismissals</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patient_dismissals.each do |patient_dismissal| %>
      <tr>
        <td><%= patient_dismissal.status %></td>
        <td><%= link_to 'Show', patient_dismissal %></td>
        <td><%= link_to 'Edit', edit_patient_dismissal_path(patient_dismissal) %></td>
        <td><%= link_to 'Destroy', patient_dismissal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient Dismissal', new_patient_dismissal_path %>
